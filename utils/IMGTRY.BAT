@ECHO OFF

:TRYCHD
REM ECHO trying %3
IMGSET %1 %3
CHKDRV %2 > NUL

IF ERRORLEVEL=1 GOTO TRYOTHER
IF ERRORLEVEL=0 GOTO ENDTRY

:TRYOTHER
REM ECHO %3 not found trying %4
IMGSET %1 %4
CHKDRV %2! ADD %4 TO /GAMES/AO486/CD/. RESTARTING.> NUL
IF ERRORLEVEL=1 GOTO RESTART
IF ERRORLEVEL=0 GOTO ENDTRY

:RESTART
IMGSET r

:ENDTRY
